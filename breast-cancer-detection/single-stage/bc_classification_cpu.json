{
    "pipeline": {
        "name": "bc_classification_cpu"
    },
    "description": "Run breast cancer classification.",
    "input": {
        "cross": [
            {
              "pfs": {
                "repo": "sample_data",
                "glob": "/*"
              }
            },
            {
              "pfs": {
                "repo": "models",
                "glob": "/"
              }
            }
          ]
    },
    "transform": {
        "cmd": [
            "/bin/bash",
            "run.sh", "cpu"
        ],
        "image": "pachyderm/breast_cancer_classifier:1.11.4"
    },
    "parallelism_spec": {
      "constant": 4
    }
}